<!DOCTYPE html>
<html lang="en">
<!--[if IE 9]>
<html class="ie9" lang="en">    <![endif]-->
<!--[if IE 8]>
<html class="ie8" lang="en">    <![endif]-->
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name=viewport content="width=device-width, initial-scale=1">

    <title>Gridus vCard | Home</title>

    <meta name="description" content="Gridus vCard HTML Template is the professional responsive personal website template, based on the latest Bootstrap 3">
    <meta name="keywords" content="html5, template, website, responsive, bootstrap">
    <meta name="author" content="neuethemes">

    <!-- CSS -->
    <link href="/Template/assets/vendor/bootstrap/css/bootstrap.min.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="/Template/assets/vendor/fontawesome/css/font-awesome.min.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="/Template/assets/vendor/flaticons/flaticon.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="/Template/assets/vendor/hover/css/hover-min.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="/Template/assets/vendor/wow/animate.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="/Template/assets/vendor/mfp/css/magnific-popup.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <!-- Remove this for disable demo panel styles -->
    <!--<link href="assets/vendor/styleselector/styleselector.css"        property='stylesheet' rel="stylesheet" type="text/css" media="screen"/>-->
    <!-- Custom styles -->
    <link href="/Template/assets/custom/css/style.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <link href="~/Template/assets/vendor/footable/footable.core.css" property='stylesheet' rel="stylesheet" type="text/css" media="screen" />
    <style>
        #preloader {
            position: fixed;
            left: 0;
            top: 0;
            z-index: 99999;
            width: 100%;
            height: 100%;
            overflow: visible;
            background: #666666 url("/Template/assets/custom/images/preloader.gif") no-repeat center center;
        }
    </style>
    @Scripts.Render("~/eddyhome/css")
</head>


<body class="boxed">
    <!--Pre-Loader-->
    <div id="preloader"></div>
    <header>

        <section id="top-navigation" class="container-fluid nopadding">

            <div class="row nopadding ident e-bg-light-texture">

                <!-- Photo -->
                <a href="#">
                    <div class="col-md-5 col-lg-4 vc-photo">&nbsp;</div>
                </a>
                <!-- /Photo -->

                <div class="col-md-7 col-lg-8 vc-name nopadding">
                    <!-- Name-Position -->
                    <div class="row nopadding name">
                        <div class="col-md-10 name-title"><h2 class="font-accident-two-light uppercase">Kim Ha Kyeong</h2></div>
                        <div class="col-md-2 nopadding name-pdf">
                            <a href="#" class="hvr-sweep-to-right"><i class="flaticon-download149" title="Download CV.pdf"></i></a>
                        </div>
                    </div>
                    <div class="row nopadding position">
                        <div class="col-md-10 position-title">

                            <section class="cd-intro">
                                <h4 class="cd-headline clip is-full-width font-accident-two-normal uppercase">
                                    <span>The Passionate </span>
                                    <span class="cd-words-wrapper">
                                        <b class="is-visible">UI/UX Designer</b>
                                        <b>C# Developer</b>
                                        <b>SQL Developer</b>
                                    </span>
                                </h4>
                            </section>

                        </div>
                        <div class="col-md-2 nopadding pdf">
                            <a href="#" class="hvr-sweep-to-right"><i class="flaticon-behance7" title="My Behance Portfolio"></i></a>
                        </div>
                    </div>
                    <!-- /Name-Position -->
                    <!-- Main Navigation -->

                    <ul id="nav" class="row nopadding cd-side-navigation">
                        <li class="col-xs-4 col-sm-2 nopadding menuitem green">
                            @*기본경로에 추가할때는 ~/를 붙여야 한다.*@
                            <a href="~/EddyHome/Index" class="hvr-sweep-to-bottom"><i class="flaticon-insignia"></i><span>home</span></a>
                        </li>
                        <li class="col-xs-4 col-sm-2 nopadding menuitem blue">
                            <a href="~/EddyHome/Resume" class="hvr-sweep-to-bottom"><i class="flaticon-graduation61"></i><span>resume</span></a>
                        </li>
                        <li class="col-xs-4 col-sm-2 nopadding menuitem cyan">
                            <a href="~/EddyHome/Portfolio" class="hvr-sweep-to-bottom"><i class="flaticon-book-bag2"></i><span>career</span></a>
                        </li>
                        <li class="col-xs-4 col-sm-2 nopadding menuitem orange">
                            <a href="~/EddyHome/Contacts" class="hvr-sweep-to-bottom"><i class="flaticon-placeholders4"></i><span>contacts</span></a>
                        </li>
                        <li class="col-xs-4 col-sm-2 nopadding menuitem red">
                            <a href="~/Member/Entry" class="hvr-sweep-to-bottom"><i class="flaticon-earphones18"></i><span>members</span></a>
                        </li>
                        <li class="col-xs-4 col-sm-2 nopadding menuitem yellow">
                            <a href="~/Board/ArticleList" class="hvr-sweep-to-bottom"><i class="flaticon-pens15"></i><span>board</span></a>
                        </li>
                    </ul>

                    <!-- /Main Navigation -->

                </div>
            </div>
        </section>

    </header>

    <div class="constant-wrap">
        @RenderBody()
    </div>

    <footer class="padding-50 e-bg-light-texture">
        <div class="container-fluid nopadding">
            <div class="row wow fadeInDown" data-wow-delay=".2s" data-wow-offset="10">
                <div class="col-md-3">
                    <h4 class="font-accident-two-normal uppercase">김 하경</h4>
                    <p class="inline-block">
                        C# , MSSQL 를 베이스 언어로 <br>
                        성장하는 개발자 입니다.
                    </p>
                    <div class="divider-dynamic"></div>
                </div>
                <div class="col-md-3 cv-link">
                    <h4 class="font-accident-two-normal uppercase">Download cv</h4>
                    <div class="dividewhite1"></div>
                    <a href="#!">English</a>
                    <a href="#!">German</a>
                    <a href="#!">Spanish</a>
                    <div class="divider-dynamic"></div>
                </div>
                <div class="col-md-3">
                    <h4 class="font-accident-two-normal uppercase">Subscribe</h4>
                    <div class="dividewhite1"></div>
                    <input>
                    <!--<p>Lorem ipsum dolor sit amet...</p>-->
                    <div class="divider-dynamic"></div>
                </div>
                <div class="col-md-3">
                    <h4 class="font-accident-two-normal uppercase">Follow me at</h4>
                    <div class="inline-block"><a href="#!"><i class="flaticon-facebook45"></i></a></div>
                    <div class="inline-block"><a href="#!"><i class="flaticon-twitter39"></i></a></div>
                    <div class="inline-block"><a href="#!"><i class="flaticon-pinterest28"></i></a></div>
                    <div class="inline-block"><a href="#!"><i class="flaticon-linkedin22"></i></a></div>
                    <div class="inline-block"><a href="#!"><i class="flaticon-vimeo22"></i></a></div>
                    <div class="inline-block"><a href="#!"><i class="flaticon-soundcloud8"></i></a></div>
                    <div class="divider-dynamic"></div>
                </div>
            </div>
            <div class="dividewhite1"></div>
            <div class="row wow fadeInDown" data-wow-delay=".4s" data-wow-offset="10">
                <div class="col-md-12 copyrights">
                    <p>© 2023 김 하경.</p>
                </div>
            </div>
        </div>
    </footer>

    <div id="image-cache" class="hidden"></div>

    <!-- JS -->
    @Scripts.Render("~/eddyhome/js")
    <script src="/Template/assets/vendor/jquery/js/jquery-2.2.0.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/imagesloaded/js/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/isotope/js/isotope.pkgd.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/mfp/js/jquery.magnific-popup.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/circle-progress/circle-progress.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/waypoints/waypoints.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/anicounter/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/wow/wow.min.js" type="text/javascript"></script>
    <script src="/Template/assets/vendor/pjax/jquery.pjax.js" type="text/javascript"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA7TwZlgMdpsnWnde7BMZhBCC35_gQ8SK8&sensor" type="text/javascript"></script>
    <!-- Remove this for disable demo panel script -->
    <!--<script src="assets/vendor/styleselector/styleselector.js"           type="text/javascript"></script>-->
    <!-- Custom scripts -->
    <script src="/Template/assets/custom/js/custom.js" type="text/javascript"></script>
    <script src="~/Template/assets/vendor/footable/footable.all.min.js" type="text/javascript"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
